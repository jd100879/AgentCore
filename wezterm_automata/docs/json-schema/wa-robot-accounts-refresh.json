{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://wezterm-automata.dev/schemas/wa-robot-accounts-refresh.json",
  "title": "WA Robot Accounts Refresh Response",
  "description": "Schema for the data field when calling 'wa robot accounts refresh'. Refreshes account usage from caut.",
  "type": "object",
  "required": ["service", "refreshed_count", "accounts"],
  "properties": {
    "service": {
      "type": "string",
      "description": "Service that was refreshed"
    },
    "refreshed_count": {
      "type": "integer",
      "minimum": 0,
      "description": "Number of accounts that had usage updated"
    },
    "refreshed_at": {
      "type": "string",
      "description": "ISO-8601 timestamp of this refresh"
    },
    "accounts": {
      "type": "array",
      "description": "Refreshed account details",
      "items": {
        "$ref": "wa-robot-accounts.json#/$defs/account_info"
      }
    }
  },
  "additionalProperties": false
}
